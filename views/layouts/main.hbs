<body>
    <div>
        <header>
            <div class="header">
                <div style="display: flex; width: 100px; ">
                    <div> <img src="/img/img_logo.png" alt="logo" class="logo"></div>
                    <div class="mb-2 mt-2 " style="font-size: large; font-weight: bolder;">YOLO</div>
                </div>
                <div class="dropdown">
                        <button class="btn btn-secondary dropdown-toggle mt-1" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Categories
                        </button>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                            <a class="dropdown-item" href="#">Web development</a>
                            <a class="dropdown-item" href="#">Mobile development</a>
                        </div>
                    </div>

                <form method="get" action=" /api/accounts/search">
                    <input type="text" class="inputsearch" name="search" placeholder="Search course..">

                    <button class="btnlogin" type="submit" value="Search">Search</button>
                </form>

                {{#if isLogin}}
               
                    <div class="dropdown">
                        <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            {{acc.detail.fullname}}
                        </button>
                        <form method="post" action="/api/accounts/logout" class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                           
                            <a class="dropdown-item" href="/api/students/profile">My Profile</a>

                            <a class="dropdown-item" href="/api/accounts/changeinfo">Edit Profile</a>
                            <a class="dropdown-item" href="/api/students/wishlist">My Wishlist</a>
                            <a class="dropdown-item" href="/api/students/shopping">My Learning</a>
                            <button class="dropdown-item" type="submit">Log Out</button>
                        </form>
                    </div>
                {{else}}
                    <div style="display: flex;">
                        <div><a href="/api/accounts/login"><button class="btnlogin">Log In</button></a></div>
                        <div><a href="/api/accounts/signup"><button class="btnsignup">Sign Up</button></a></div>
                    </div>                
                {{/if}}

        </header>

        <div>
            {{{body}}}

        </div>

        <div class='footer'>

            <div class='father'>
                <div class='son'>
                    <div className='logo' style="display: flex;">
